#include <iostream>
#include <string>
using namespace std;

string elements[20];
string subset[20];
int n;

void powerSet(int index, int subsetSize) {
    if (index == n) {
        cout << "(";
        for (int i = 0; i < subsetSize; i++) {
            if (i) cout << ",";
            cout << subset[i];
        }
        cout << ")";
        cout << " ";
        return;
    }
    powerSet(index + 1, subsetSize);
    subset[subsetSize] = elements[index];
    powerSet(index + 1, subsetSize + 1);
}

int main() {
    cout << "請輸入元素個數 n：";
    cin >> n;

    cout << "請輸入 " << n << " 個元素：";
    for (int i = 0; i < n; i++)
        cin >> elements[i];

    cout << "Powerset 結果如下：" << endl;
    powerSet(0, 0);

    return 0;
}
